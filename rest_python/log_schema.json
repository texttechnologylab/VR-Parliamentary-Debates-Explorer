{
  "Schema": [
    {
      "$id": "",
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "title": "Player Log into a Scene",
      "properties": {
        "_id": {
          "description": "Generated hash of MongoDB, for a Player unique for every player in a Scene",
          "type": "String"
        },
        "playerId": {
          "description": "Generated ID for the player in a scene",
          "type": "String"
        },
        "roomId": {
          "description": "Generated hash of ubiq-Server for a Scene",
          "type": "String"
        },
        "sceneId": {
          "description": "ID of the Scene",
          "type": "String"
        },
        "clientId": {
          "description": "ID of the client generated from the ubiq-Server",
          "type": "String"
        },
        "localTime": {
          "description": "Time in Sec of the device",
          "type": "int"
        },
        "serverTimestamp": {
          "description": "TimesTamp of the log-Server",
          "type": "String"
        }
      },
      "required": [
        "_id",
        "playerId",
        "roomId",
        "sceneId",
        "clientId",
        "localTime",
        "serverTimestamp",
        "messageId"
      ]
    },
    {
