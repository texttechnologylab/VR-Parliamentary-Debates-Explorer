        "messageId": {
          "description": "Actual Message of the Player",
          "type": "int"
        },
        "serverTimestamp": {
          "description": "TimesTamp of the log-Server",
          "type": "String"
        }
      },
      "required": [
        "_id",
        "playerId",
        "audioData",
        "localTime",
        "messageId"
